{
  "short_name": "Github History",
  "content_security_policy": "script-src 'self' 'sha256-YcQmyTC2Q4VraVXZkgUnBC8G5J1IWA9tI21c+Jy8L24='; object-src 'self'",
  "name": "Github History",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Github History",
    "default_icon": "favicon.ico"
  },
  "background": {
    "scripts": [
      "backgroundScripts/background.js"
    ],
    "persistent": true
  },
  "permissions": [
  ],
  "web_accessible_resources": ["index.html"],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/"
      ],
      "js": [
        "inject/inject.js"
      ]
    }
  ],
  "debug": true,
  "version": "1.0"
}
